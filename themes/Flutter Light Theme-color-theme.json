	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ada9a9"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#ada9a9"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#2F5A92"
			}
		},
		{
			"name": "Modifiers",
			"scope": [
				"storage.modifier",
			],
			"settings": {
				"foreground": "#2271D8"
			}
		},
		{
			"name": "declaration",
			"scope": [
				"meta.declaration",
			],
			"settings": {
				"foreground": "#0065DA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "declaration",
			"scope": [
				"punctuation.terminator",
			],
			"settings": {
				"foreground": "#212322",
			}
		},
		{
			"name": "interpolated string",
			"scope": [
				"string.interpolated.single",
			],
			"settings": {
				"foreground": "#016336",
				"fontStyle": ""
			}
		},
		{
			"name": "keyword declaration",
			"scope": [
				"keyword.declaration",
			],
			"settings": {
				"foreground": "#0065DA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "keyword declaration",
			"scope": [
				"support.class",
			],
			"settings": {
				"foreground": "#007c00",
			}
		},
		{
			"name": "keyword declaration",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#1a73e8",
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#A52A2A"
			}
		},
		{
			"name": "Variables",
